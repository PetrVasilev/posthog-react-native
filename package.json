{
  "name": "posthog-react-native",
  "version": "1.1.6",
  "description": "The hassle-free way to add PostHog to your React-Native app.",
  "license": "MIT",
  "main": "build/cjs/index.js",
  "module": "build/esm/index.js",
  "types": "build/esm/index.d.ts",
  "files": [
    "android",
    "ios",
    "build",
    "src",
    "RNPostHog.podspec"
  ],
  "scripts": {
